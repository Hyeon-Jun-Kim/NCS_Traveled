<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hanshin.ncs_travled">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:installLocation="preferExternal"

        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HT_Activity" android:label="Home"> </activity>
        <activity android:name=".HT_CallBookList" android:label="CallBooks"> </activity>
        <activity android:name=".HT_Result" android:label="Gallery"> </activity>
        <activity android:name=".BT_Activity" android:label="Book"> </activity>
        <activity android:name=".CT_Activity" android:label="Community"> </activity>
        <activity android:name=".CT_Create" android:label="Community"> </activity>
        <activity android:name=".SettingActivity" android:label="google login"> </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

</manifest>